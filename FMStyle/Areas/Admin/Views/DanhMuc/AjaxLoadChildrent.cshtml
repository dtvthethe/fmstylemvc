@using FMStyle.Areas.Admin.Models;
@using FMStyle.Libraries;
@model List<danhmuc>
@for (int i=0; i< Model.Count; i++)
{
    <tr>
        <td class="stt">
            <input id="" type="checkbox" class="ckhChil" name="@i&#46;chil" value="@Model[i].id_danhmuc">
            <input type="hidden" name="hdChilCount" value="@i" />
        </td>
        <td class="stt"><span>@(i + 1)</span></td>
        <td class="hinhanh"><a href="/admin/danh-muc-thong-tin-chil-@Lib.VietNamToEnglish(Model[i].ten_danhmuc)-@Model[i].id_danhmuc&#46;html">@Model[i].ten_danhmuc</a></td>
        <td class="hinhanh"></td>
    </tr>
}
